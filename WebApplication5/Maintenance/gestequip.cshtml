									<td class="column66 text-center">
										<div>
											@Html.TextBox("file", "", new { type = "file" })
											<input type="submit" value="Save Files" />
											@ViewBag.Message
										</div>
									</td>}
									<td class="column77 text-center">@item.Historique_de_réparation</td>
									<td class="column88 text-center">

										<a href="@Url.Action(" Edit", "mainten" )" class="ml-3 align-text"><i class="fas fa-pen-square fa-2x "></i> </a>
