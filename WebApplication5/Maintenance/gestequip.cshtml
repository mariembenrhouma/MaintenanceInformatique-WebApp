									<th class="column44 text-center">Libelle</th>
									<th class="column55 text-center">Type</th>
									<th class="column66 text-center">Fiche Technique</th>
									<th class="column77 text-center">H. des répartitions</th>
									<th class="column88 text-center ">Ajouter</th>
								</tr>
							</thead>
							<tbody>

