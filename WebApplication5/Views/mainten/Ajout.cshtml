@model WebApplication5.Models.class2

@{
    ViewBag.Title = "Ajout";
    Layout = "~/Views/mainten/_Layoutmainte.cshtml";
}

<h2>Ajout</h2>


@using (Html.BeginForm("Ajout", "mainten", FormMethod.Post))
{
    @Html.TextBoxFor(x => x.id_equip, new { @class = "form-control", @placeholder = "Numéro" })
    @Html.TextBoxFor(x => x.id_dep, new { @class = "form-control", @placeholder = "id depa" })
    @Html.TextBoxFor(x => x.libelledep, new { @class = "form-control", @placeholder = "Département" })
    @Html.TextBoxFor(x => x.@ref, new { @class = "form-control", @placeholder = "Référence" })
    @Html.TextBoxFor(x => x.libelleq, new { @class = "form-control", @placeholder = "Libelle" })
    @Html.TextBoxFor(x => x.id_type, new { @class = "form-control", @placeholder = "id type" })
    @Html.TextBoxFor(x => x.etat, new { @class = "form-control", @placeholder = "etat" })
    @Html.TextBoxFor(x => x.denomination, new { @class = "form-control", @placeholder = "Type" })
    @Html.TextBoxFor(x => x.id_fiche, new { @class = "form-control", @placeholder = "Fiche technique" })
    @Html.TextBoxFor(x => x.id_typentr, new { @class = "form-control", @placeholder = "id type entretien" })

    @Html.TextBoxFor(x => x.libellentr, new { @class = "form-control", @placeholder = "Historique de reparation" })
    <input type="submit" value="submit" class="btn btn-block btn-danger" />
    <br />

    <div>
        <br />
        @Html.ActionLink("Back ", "gestequip")
    </div>

    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
}
